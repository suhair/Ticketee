<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
  <% flash.each do |k, v| %>
    <div class='flash' id='<%= k %>'>
      <%= v %>
    </div>
  <% end %>
<%= yield %>

</body>
</html>
